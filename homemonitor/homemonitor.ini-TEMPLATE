; Copy this file to your HOME directory.
; cp homemonitor.ini-TEMPLATE ~/.homemonitor.ini
; chmod 600 ~/.homemonitor.ini
;
; In the mail section, add your user/password that
; will be used to send email.
; Add who you want to send email to.

[mail]
user=sender@gmail.com
password=<application password>
to=receiver1@gmail.com, receiver2@gmail.com
server=smtp.gmail.com
port=587

[internet]
server=google.com
port=80
timeout_in_seconds=20
